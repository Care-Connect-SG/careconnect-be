{
  "version": 2,
  "installCommand": "pip install -r requirements.txt && uvicorn main:app --host 127.0.0.1 --port 8000",
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "env": {
    "APP_MODULE": "main:app"
  }
}
